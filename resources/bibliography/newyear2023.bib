@Article{Guix1999,
  author      = {Guix, J. C. and Martín, M. and Manõsa, S.},
  journal     = {Biodiversity and Conservation},
  title       = {Conservation status of parrot populations in an {A}tlantic rainforest area of southeastern {B}razil},
